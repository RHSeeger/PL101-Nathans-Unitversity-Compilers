start = whitespace* item:(value / quoted) whitespace*
        { return item; }

value = (atom / expression)

whitespace =
    [ \t\n]

validchar
    = [0-9a-zA-Z_?!+\-=@#$%^&*/.]

atom =
    chars:validchar+
        { return chars.join(""); }

expression =
    "(" first:start+ ")"
        { return first; }

quoted = "'" item:value
    { return ["quote", item]; }
