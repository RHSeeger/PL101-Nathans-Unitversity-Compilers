start =
    atom
    / expression

space =
	[ ]

validchar
    = [0-9a-zA-Z_?!+\-=@#$%^&*/.]

atom =
    chars:validchar+
        { return chars.join(""); }

expression =
    "(" first:start rest:( space start )* ")"
 	{ 
            return( [first].concat( 
                rest.map(function(item) { return item[1]; }) 
            ) );
        }
